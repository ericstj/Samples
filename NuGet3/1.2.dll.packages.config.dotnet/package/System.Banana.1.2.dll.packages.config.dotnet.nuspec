<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>System.Banana.1.2.dll.packages.config.dotnet</id>
    <version>1.0.0</version>
    <authors>NugetSamples</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Sample package</description>
    <dependencies>
      <group targetFramework="dotnet">
        <dependency id="System.IO" version="4.0.0" />
        <dependency id="System.Reflection" version="4.0.0" />
        <dependency id="System.Resources.ResourceManager" version="4.0.0" />
        <dependency id="System.Runtime" version="4.0.0" />
      </group>
      <!-- all depdencies are inbox in netcore45 / win8 -->
      <group targetFramework="netcore45" />
      <!-- netcore50 dependencies from nuget packages -->
      <group targetFramework="netcore50">
        <dependency id="System.IO" version="4.0.0" />
        <dependency id="System.Reflection" version="4.0.0" />
        <dependency id="System.Resources.ResourceManager" version="4.0.0" />
        <dependency id="System.Runtime" version="4.0.0" />
      </group>
      <!-- more inbox frameworks -->
      <group targetFramework="net45" />
      <group targetFramework="wp8" />
      <group targetFramework="wpa81" />
      <!-- required for pcl projects that don't use project.json with dotnet -->
      <group targetFramework="portable-netcore45+net45+wp8+wpa8" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\src\System.Banana\bin\Debug\System.Banana.dll" target="lib/dotnet" />
    <file src="..\src\System.Banana\bin\Debug\System.Banana.dll" target="lib/portable-netcore45+net45+wp8+wpa8" />
  </files>
</package>